<div class="todo">
    <% groupedTasks.forEach(machine => { %>
        <div class="card p-2 mt-2">
            <h3><a href="machines/<%= machine.tasks[0].machine.id%>"><%=  machine.tasks[0].machine.name %></a> </h3> 
            <%- include('tasks.ejs', {tasks:machine.tasks })%>
        </div>
    <% }) %>
</div>